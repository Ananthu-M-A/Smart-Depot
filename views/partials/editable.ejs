<!-- Products Start -->
<div class="container">
    <div class="row">
      <% products.forEach(product => { %>
        <% if(!product.blocked) { %>
        <div class="col-md-3">
          <div class="card mb-3">
            <img src="/productImage/<%= product.productImageNames[0] %>" class="card-img-top" id="productImageLarge" alt="Product Image">
            <div class="card-body">
              <h5 class="card-title text-center"><%= product.productName %></h5>
              <p class="card-text">Category: <%= product.productCategory %></p>
              <p class="card-text">Brand: <%= product.productBrand %></p>
              <p class="card-text">Price: <%= product.productPrice %></p>
              <!-- Add other product details as needed -->
              <a href="/home/productDetails/<%= product._id %>" class="btn btn-primary">View Details</a>
            </div>
          </div>
        </div>
      <% }}); %>
    </div>
  </div>
  
<!-- Products End -->

